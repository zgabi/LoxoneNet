{
  "Settings": {
    "LogLevel": "Information",
    "Hostname": "yourhostname.dyndns.org", // can be any hostname, not only dyndns
    "HttpsPort": 8082, // the prot of the google home service
    "HttpPort": 80, // for Let's encrypt
    "GoogleClientId": "", // set this to the configured OAuth Client ID or keep empty to ignore the check (less secure)
    "GoogleClientSecret": "", // set this to the configured OAuth Client Secret or keep empty to ignore the check (less secure)
    "GoogleAccessToken": "random1", // put any random alphanumeric token here
    "GoogleRefreshToken": "random2", // put any random alphanumeric token here
    "LetsEncryptEmail": "youremail@example.com", // basically does not matter, Let's Encrypt will send some emails maybe to this address. Probably a valid email address (format) needed, but I think it can be anything.
    "CertCountry": "HU",
    "CertState": "Hungary",
    "CertLocality": "Budapest",
    "CertOrganization": "MyCertOrg",
    "CertOrganizationUnit": "MyCertOrgUnit",
    //"DsmrReaderAddress": "p1reader.lan",
    //"DsmrReaderPort": 9999,
    //"GrowattServerHost": "server.growatt.com",
    //"GrowattUserName": "",
    //"GrowattPassword": "",
    "LoxoneUrl": "loxone.lan",
    "LoxoneUserName": "googlehome", // this User should be added to your Loxone configuration 
    "LoxonePassword": "P@assword1234" // the password for the previous Loxone user
  }
}
